{
  "name": "live-russia-tester-dashboard",
  "version": "1.0.0",
  "description": "Web application for managing LIVE RUSSIA mobile project testers",
  "main": "backend/server.js",
  "scripts": {
    "start": "node backend/server.js",
    "start:prod": "NODE_ENV=production node backend/server.js",
    "dev": "nodemon backend/server.js",
    "migrate": "node backend/db/run-migrations.js",
    "rollback": "node backend/db/rollback-migrations.js",
    "seed": "node backend/db/seed-admin.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "setup": "node setup-check.js",
    "checkpoint": "node checkpoint-verification.js",
    "quick-start": "node quick-start.js",
    "docker:build": "docker build -t live-russia-dashboard .",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:restart": "docker-compose restart",
    "pm2:start": "pm2 start ecosystem.config.js --env production",
    "pm2:stop": "pm2 stop live-russia-dashboard",
    "pm2:restart": "pm2 restart live-russia-dashboard",
    "pm2:logs": "pm2 logs live-russia-dashboard",
    "pm2:monit": "pm2 monit"
  },
  "keywords": [
    "dashboard",
    "testers",
    "bug-tracking",
    "google-sheets"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "bcrypt": "^5.1.1",
    "express-session": "^1.17.3",
    "connect-redis": "^7.1.0",
    "googleapis": "^128.0.0",
    "multer": "^1.4.5-lts.1",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11"
  }
}
